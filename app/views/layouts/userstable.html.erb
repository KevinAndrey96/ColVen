<h1>Usuarios</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table class="table">
    <thead>
        <tr>
        <td>Email</td>
        <td>Rol</td>
        <td>Fecha Creación</td>
        <td>Ultima actualización</td>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
          <tr>
          <td><%= user.email %></td>
          <td><%= user.role %></td>
          <td><%= user.created_at %></td>
          <td><%= user.updated_at %></td>
          </tr>
        <% end %>
    </tbody>
</table>
